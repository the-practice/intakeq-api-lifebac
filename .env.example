# Environment Configuration for IntakeQ Voice Assistant

# IntakeQ API Configuration
INTAKEQ_API_KEY=your-intakeq-api-key-here

# Server Configuration
PORT=3000
NODE_ENV=development

# Voice Assistant Configuration
DEFAULT_PRACTITIONER_EMAIL=doctor@yourpractice.com
DEFAULT_SERVICE_ID=your-default-service-id
DEFAULT_LOCATION_ID=1

# Business Hours (24-hour format)
BUSINESS_HOURS_START=09:00
BUSINESS_HOURS_END=17:00
BUSINESS_DAYS=1,2,3,4,5

# Phone Transfer Configuration
TRANSFER_PHONE_NUMBER=+15551234567

# Security (Optional - for production)
ALLOWED_ORIGINS=https://api.bland.ai,https://bland.ai

# Logging
LOG_LEVEL=info