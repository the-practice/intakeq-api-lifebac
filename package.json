{
  "name": "@lifebac/intakeq",
  "version": "1.3.8",
  "description": "The IntakeQ API js Client",
  "main": "dist/bundle.js",
  "module": "dist/bundle.esm.js",
  "browser": "dist/bundle.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "./scripts/build.sh",
    "dev": "./scripts/dev.sh",
    "voice-server": "node -r dotenv/config dist/voice-server.js",
    "voice-dev": "ts-node -r dotenv/config src/voice-server.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "publish:npm": "cp .npmrc.publish .npmrc && npm publish && git checkout .npmrc"
  },
  "repository": "git://github.com/lifebac/intakeq-api.git",
  "author": "LifeBac Developers <developer@lifebac.com>",
  "contributors": [
    "Justin Rahardjo <hello@justinrahardjo.com>",
    "Patrick Eaton <patrick.eaton.04@gmail.com"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LifeBac/intakeq-api/issues"
  },
  "homepage": "https://github.com/LifeBac/intakeq-api#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "dts-bundle": "^0.7.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "husky": "^7.0.0",
    "jest": "27.2.1",
    "lint-staged": "^10.1.0",
    "prettier": "^2.0.2",
    "rollup": "^2.3.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "27.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
